{
    "name": "@refkinscallv/express-routing-ts",
    "version": "1.0.5",
    "description": "Laravel-style routing for Express.js using TypeScript",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "files": ["dist"],
    "scripts": {
        "build": "tsc",
        "dev": "nodemon --config nodemon.json",
        "start": "node dist/index.js",
        "prepublishOnly": "npm run build",
        "prepare": "npm run build"
    },
    "keywords": [
        "express",
        "routing",
        "typescript",
        "laravel-style",
        "express-routing",
        "express-route",
        "express-library",
        "typescript-library"
    ],
    "author": "Refkinscallv",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/refkinscallv/express-routing-ts.git"
    },
    "bugs": {
        "url": "https://github.com/refkinscallv/express-routing-ts/issues"
    },
    "homepage": "https://github.com/refkinscallv/express-routing-ts#readme",
    "dependencies": {
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^20.0.0",
        "nodemon": "^3.1.9",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.0"
    }
}  